/* pages/items/items.wxss */
@font-face {
    font-family: 'iconfont';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: normal;
    font-style: normal;
}
.iconfont{
  font-family: "iconfont" !important;
  font-size: 20px;
  font-style: normal;
}
.iconfont.icon-wenti2{
  font-size:40px;
  color:#24aee1;
  width:150rpx;
  text-align:center;
}
.icon-wenti2::before{
  content: "\e6b0";
}
.icon-fabulous::before{
  content: "\e605";
}
.icon-zan-copy::before{
  content: "\e609";
}
.icon-liuyan::before{
  content: "\e638";
}
.container {
  background-color: #F2f2f2;
  min-height: 100%;
  padding-bottom: 100rpx;
  font-family: 'iconfont';
}
.items{
  display:flex;
  width:95%;
  height:150rpx;
  align-items:stretch;
  line-height:150rpx;
  border:1px solid #ddd;
  background:#fff;
  left:50%;
  position:relative;
  transform:translateX(-50%);
  margin-top: 20rpx;
}
.item-info-detail{
  width:100%;
font-size:20px;
height:150rpx;
display:flex;
flex-direction:column;
justify-content:center;
padding-left:35rpx;
margin-top:30rpx;
}
.item-name{
  margin-top: 30rpx;
}
.item-number{
  width:250rpx;
  font-size:23px;
}
.item-icon{
  flex: 1;
}