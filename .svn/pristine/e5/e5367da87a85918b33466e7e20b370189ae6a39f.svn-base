/* pages/group/group.wxss */
/* pages/dinner/dinner.wxss */
.container {
  background-color: #fff;
  min-height: 100%;
  /* padding-bottom: 100rpx; */
}
.ces-logo{
  width: 100%;
  height: 100rpx;
}
.meeting-title{
  position:absolute;
  text-align:center;
  width:100%;
  height:100rpx;
  line-height:100rpx;
  color:#fff;
  font-size:36rpx;
  top: 0;
  left: 0;
}
.meeting-bg{
  width: 100%;
  position:absolute;
  height:100%;
}
.dinner-content{
  flex: 1;
}
.dinner-group{
  margin: 30rpx 30rpx;
  display:flex;
  justify-content:flex-start;
  align-items:center;
  border-bottom:1px solid #f04b12;
  padding-bottom:8rpx;
}
.dinner-text{
  width:140rpx;
  font-size:45rpx;
  color:#f04b12;
}
.dinner-scroll{
  height: 1100rpx;
}
.dinner-img{
  width:45rpx;
  height:35rpx;
  padding-left:10rpx;
}
.dinner-users{
  display:flex;
  justify-content:flex-start;
  align-items:stretch;
  align-content:stretch;
  flex-wrap:wrap;
  text-align:center;
}
.dinner-user{
  width:150rpx;
  height:110rpx;
  margin-left:13px;
  margin-bottom:25rpx;
  border-radius:15rpx;
  border:1px solid #ddd;
  line-height: 110rpx;
  font-size: 32rpx;
}
.footer-box{
  height:120rpx;
  display:flex;
  width:100%;
  position:fixed;
  bottom:0;
  left:0;
  border-top: 1px solid #b79f9f;
}
.menu-bg{
  position: absolute;
  width: 100%;
  height:100%;
}
.active-img{
  position:absolute;
  left:0;
  right:0;
  top:0;
  bottom:0;
  z-index:-1;
  width:100%;
}
.footer-nav.active{
  position: relative;
}
.footer-nav.active text{
  color: #fff;
}
.nav-guide{
  width:50rpx;
  height:50rpx;
  display:block;
  left:50%;
  margin-left:-25rpx;
  position:relative;

}
.footer-nav text{
  display:block;
  flex:1;
  text-align:center;
  color: #807b75;
  margin-top:10rpx;
}
.footer-nav{
  height:120rpx;
  flex:1;
  text-align:center;
  font-size:29rpx;

  z-index:10;
  padding-top:15rpx;
}
.footer-nav:not(:last-child){
  border-right:1px solid #fff;
}